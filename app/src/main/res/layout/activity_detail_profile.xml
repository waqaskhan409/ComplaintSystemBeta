<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
xmlns:android="http://schemas.android.com/apk/res-auto"
xmlns:android1="http://schemas.android.com/apk/res/android"
tools:ignore="NamespaceTypo"
android1:layout_height="wrap_content"
android1:layout_width="wrap_content">

    <include
        layout="@layout/app_bar_main"
        android1:layout_width="match_parent"
        android1:layout_height="673dp" />
<ScrollView
    android1:layout_width="wrap_content"
    android1:layout_height="wrap_content"
    android1:layout_marginTop="?android:attr/actionBarSize"

    >
<RelativeLayout android1:layout_width="match_parent"
    android1:layout_height="wrap_content"
    >
    <LinearLayout
        android1:layout_width="match_parent"
        android1:orientation="vertical"
        android1:layout_height="wrap_content">
        <LinearLayout
            android1:layout_width="match_parent"
            android1:layout_height="wrap_content"
            android1:layout_marginLeft="50dp"
            android1:layout_marginRight="50dp"
            android1:layout_marginTop="30dp"
            android1:orientation="horizontal">
            <de.hdodenhof.circleimageview.CircleImageView
                android1:layout_width="80dp"
                android1:layout_height="80dp"
                android1:id="@+id/deepee"
                android1:src="@drawable/account_image"
                ></de.hdodenhof.circleimageview.CircleImageView>
            <!-- <ImageView
                 android1:layout_width="wrap_content"
                 android1:layout_height="wrap_content"
                 android1:layout_marginLeft="-23dp"
                 android1:layout_marginBottom="14dp"
                 android1:src="@drawable/add">

             </ImageView>-->

            <LinearLayout
                android1:layout_width="wrap_content"
                android1:layout_height="match_parent"
                android1:layout_marginLeft="50dp"
                android1:gravity="center_vertical"
                android1:orientation="vertical">

                <TextView
                    android1:layout_width="wrap_content"
                    android1:layout_height="wrap_content"
                    android1:text="Name"
                    android1:id="@+id/name"
                    android1:textColor="@color/black"
                    android1:textSize="20sp"></TextView>

                <TextView
                    android1:layout_width="wrap_content"
                    android1:layout_height="wrap_content"
                    android1:layout_marginTop="10dp"
                    android1:id="@+id/accountNumber"
                    android1:text="CNIC number"></TextView>
            </LinearLayout>


        </LinearLayout>
        <View
            android1:layout_width="match_parent"
            android1:layout_height="1dp"
            android1:layout_marginTop="20dp"
            android1:layout_marginLeft="40dp"
            android1:layout_marginRight="40dp"
            android1:background="@color/colorAccent"
            />
        <TextView
            android1:layout_width="match_parent"
            android1:layout_height="50dp"
            android1:id="@+id/email"
            android1:layout_marginTop="20dp"
            android1:layout_marginLeft="20dp"
            android1:layout_marginRight="20dp"
            android1:gravity="center_vertical"
            android1:textColor="@color/black"
            android1:textSize="20dp"
            android1:hint="@string/user_email"
            android1:paddingLeft="10dp"
            android1:paddingRight="10dp"
            android1:background="@drawable/layout_bg"
            ></TextView>
        <TextView
            android1:layout_width="match_parent"
            android1:layout_height="50dp"
            android1:id="@+id/fatherName"
            android1:layout_marginTop="20dp"
            android1:textColor="@color/black"
            android1:layout_marginLeft="20dp"
            android1:enabled="false"
            android1:layout_marginRight="20dp"
            android1:gravity="center_vertical"
            android1:textSize="20dp"
            android1:hint="Father's Name"
            android1:paddingLeft="10dp"
            android1:paddingRight="10dp"
            android1:background="@drawable/layout_bg"
            ></TextView>
        <TextView
            android1:layout_width="match_parent"
            android1:layout_height="50dp"
            android1:layout_marginTop="20dp"
            android1:layout_marginLeft="20dp"
            android1:layout_marginRight="20dp"
            android1:textColor="@color/black"
            android1:hint="Appointed date"
            android1:id="@+id/appointedDate"
            android1:gravity="center_vertical"
            android1:textSize="20dp"
            android1:enabled="false"
            android1:paddingLeft="10dp"
            android1:paddingRight="10dp"
            android1:background="@drawable/layout_bg"
            ></TextView>
        <TextView
            android1:layout_width="match_parent"
            android1:layout_height="50dp"
            android1:layout_marginTop="20dp"
            android1:layout_marginLeft="20dp"
            android1:layout_marginRight="20dp"
            android1:hint="Birth date"
            android1:id="@+id/birthDate"
            android1:gravity="center_vertical"
            android1:textColor="@color/black"
            android1:textSize="20dp"
            android1:enabled="false"
            android1:paddingLeft="10dp"
            android1:paddingRight="10dp"
            android1:background="@drawable/layout_bg"
            ></TextView>
        <TextView
            android1:layout_width="match_parent"
            android1:layout_height="50dp"
            android1:layout_marginTop="20dp"
            android1:layout_marginLeft="20dp"
            android1:layout_marginRight="20dp"
            android1:textColor="@color/black"
            android1:hint="Gender"
            android1:id="@+id/gender"
            android1:gravity="center_vertical"
            android1:textSize="20dp"
            android1:enabled="false"
            android1:paddingLeft="10dp"
            android1:paddingRight="10dp"
            android1:background="@drawable/layout_bg"
            ></TextView>
        <TextView
            android1:layout_width="match_parent"
            android1:layout_height="50dp"
            android1:layout_marginTop="20dp"
            android1:textColor="@color/black"
            android1:layout_marginLeft="20dp"
            android1:layout_marginRight="20dp"
            android1:hint="local"
            android1:id="@+id/local"
            android1:gravity="center_vertical"
            android1:textSize="20dp"
            android1:enabled="false"
            android1:paddingLeft="10dp"
            android1:paddingRight="10dp"
            android1:background="@drawable/layout_bg"
            ></TextView>
        <TextView
            android1:layout_width="match_parent"
            android1:layout_height="50dp"
            android1:layout_marginTop="20dp"
            android1:layout_marginLeft="20dp"
            android1:layout_marginRight="20dp"
            android1:textColor="@color/black"
            android1:hint="Last update"
            android1:id="@+id/lastUpdate"
            android1:gravity="center_vertical"
            android1:textSize="20dp"
            android1:enabled="false"
            android1:paddingLeft="10dp"
            android1:paddingRight="10dp"
            android1:background="@drawable/layout_bg"
            ></TextView>

    </LinearLayout>
</RelativeLayout>
</ScrollView>
</RelativeLayout>